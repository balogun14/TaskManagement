@model BlogApplication.ViewModels.AuthViewModel.EditView
@{
    ViewData["Title"] = "Edit Profile";
}

<form method="post" >
  <div class="mb-3">
    <label asp-for="Username" class="form-label"></label>
    <input asp-for='Username' type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" >
  </div>
    <div class="mb-3">
    <label asp-for="Email" class="form-label"></label>
    <input asp-for='Email' type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" >
  </div>
  <div class="mb-3">
    <label asp-for="Password" class="form-label"></label>
    <input asp-for="Password"  type="password" class="form-control" id="exampleInputPassword1">
  </div>
    <div class="mb-3">
    <label asp-for="Phone" class="form-label"></label>
    <input asp-for="Phone"  type="tel" class="form-control" id="exampleInputPassword1">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>